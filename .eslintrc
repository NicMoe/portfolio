{
  "env": {
    "browser": true
  },

  "extends": [
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },

  "plugins": ["react", "jsx-a11y"],

  "rules": {
    "react/jsx-props-no-spreading": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-key": 2
  }
}
